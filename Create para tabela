CREATE TABLE ALUNO (
 ID INT NOT NULL IDENTITY,
 NOME VARCHAR(50) NOT NULL,
 ENDERECO VARCHAR(50),
 CEP VARCHAR(9),
 BAIRRO VARCHAR(50),
 CIDADE VARCHAR(50),
 UF VARCHAR(2),
 TELEFONE VARCHAR(15),
MATRICULA VARCHAR(20),
DATA_DE_NASCIMENTO DATETIME(10),
EMAIL(50),
 CONSTRAINT PK_ALUNO PRIMARY KEY(ID)
)
